{
  "/register": {
    "POST": {
      "$.request": {
        "$.body": {
          "name": "John Doe"
        }
      },
      "$.response": {
        "$.body": {
          "name": "John Doe",
          "age": 30,
          "address": {
            "street": "123 Main St",
            "city": "Anytown",
            "state": "CA",
            "zip": "12345"
          }
        }
      }
    }
  },
  "/video": {
    "GET": {
      "$.response": {
        "$.content-type": "video/mp4",
        "$.file": "./sample.mp4"
      }
    }
  },
  "/image": {
    "GET": {
      "$.response": {
        "$.content-type": "image/webp",
        "$.file": "https://kanto.legiaodosherois.com.br/w728-h381-gnw-cfill-gcc-f:fbcover/wp-content/uploads/2022/06/legiao_ZjNxhF03Tgso.jpg.webp"
      }
    }
  }
}
